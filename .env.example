# Self-Hosted AI Toolkit - Environment Variables
# Copy this file to .env and edit the values as needed

# General Settings
DOMAIN=mac.local

# n8n Settings
N8N_ENCRYPTION_KEY=change_this_to_a_random_string
POSTGRES_PASSWORD=change_this_to_a_secure_password
N8N_PORT=5678

# Ollama Integration
OLLAMA_HOST=host.docker.internal
OLLAMA_PORT=11434

# Traefik Settings
TRAEFIK_DASHBOARD_PORT=8080

# Astroluma Settings
ASTROLUMA_PORT=80

# Agent Zero Settings
AGENT_ZERO_PORT=80

# Synology Backup Settings
NAS_USER=admin
NAS_IP=192.168.1.x
BACKUP_DIR=/volume1/docker_backups
